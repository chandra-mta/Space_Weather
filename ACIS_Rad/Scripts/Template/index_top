<script language="JavaScript">
    function MyWindowOpener(imgname) {
        msgWindow=open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=yes,status=no,width=810,height=1010,resize=yes");
        msgWindow.document.close();
        msgWindow.document.write("<html><head><title>Radiaton  plot:   "+imgname+"</title></head>");
        msgWindow.document.write("<body bgcolor='white'>");
        msgWindow.document.write("<img src='./Plots/"+imgname+"' border=0 width=800 height=1000></body></html>");
        msgWindow.focus();
    }
    function MyWindowOpener2(imgname) {
        msgWindow=open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=yes,status=no,width=800,height=410,resize=yes");
        msgWindow.document.close();
        msgWindow.document.write("<html><head><title>Radiaton  plot:   "+imgname+"</title></head>");
        msgWindow.document.write("<body bgcolor='white'>");
        msgWindow.document.write("<img src='./Plots/"+imgname+"' border=0 width=800 height=400></body></html>");
        msgWindow.focus();
    }
</script>


